image{
  background: pink
}
[class*="icon-"] {
    font-family: "iconfont" !important;
}
page{
  height: 100%;
}
.nodata_text{
  color: #666666;
  font-size: 14px;  
  text-align: center;  
  line-height: 100vh;
} 
.searchBar{
  width: 100%;
  height: 120rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background: white;
}
.searchBar input{
  width: 88%;
  height: 80rpx;
  margin:20rpx 0 25rpx 5%; 
  background: #efefef;
  padding-left: 20rpx;
}
.searchBar view{
  width: 70%;
  height: 80rpx;
  padding-left:32rpx;
  position: relative;
  background: #efefef;
  left: 5%;
  top: -100rpx;
  z-index: 10;
}
.searchBar view icon{
  position: absolute;
  top: 26rpx;
}
.searchBar view text{
  margin-left: 48rpx;
  font-size: 32rpx;
  color: #888;
  line-height: 80rpx;
}
.searchBar image{
  width: 46rpx;
  height: 50rpx;
  position: absolute;
  top: 40rpx;
  right: 8%;
}


/* box */
.box{
  width: 100%;
  height: calc(100vh - 50px);
  overflow: hidden;
  /* margin-left: 2%; */
  /* margin-left: 5%;
  margin-top:110rpx;  */
}
/* left */
.Bleft{
  width: 25%;
  height: 100%;
  float: left;
  /* margin-left:-6%; */
  background: #f5f5f5;
  /* position: fixed; */
  overflow-y: auto;
}
.Bleft view{
  width: 100%;
  height: 120rpx;
}
.Bleft view text{
  display: block;
  width: 169rpx;
  height: 60rpx;
  margin-top:30rpx;
  float: right;
  line-height: 60rpx;
  text-align: center;
}
/* 点击样式 */
.BleftView{
  width: 100%;
  height: 120rpx;
  background: white;
}
.BleftView Text{
  display: block;
  width: 169rpx;
  height: 56rpx;
  margin-top:30rpx;
  float: right;
  line-height: 56rpx;
  text-align: center;
  color: #f4a040;
  border-right: 6rpx solid #f4a040;
}
/* 样式集 */

/* right */
.Bright{
  width: 75%;
  height: 100%;
  float: right;
  overflow-y: auto;
}
.Bright image{
  width: 100%;
  height: 300rpx;
}
.BrightTXT{
  width: 100%;
  height: 80rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 38rpx;
  font-weight: bold;
}
.content{
  width: 100%;
  height: auto;
  display: block;
}
.content view{
  width: calc(33.33% - 24rpx);
  height: 200rpx;
  float: left;
  margin: 12rpx 12rpx 0 12rpx;
  /* margin:12rpx 0 0 3%; */
  text-align: center;
}
.content view image{
  width: 100%;
  height: 140rpx;
  margin-bottom:8rpx; 
}
.content view text{
  font-size: 24rpx;
  display: block;
  overflow:hidden; 
  text-overflow:ellipsis; 
  white-space:nowrap; 
}