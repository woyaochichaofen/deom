.head{
  padding:40rpx 20rpx;
  color: #999;
  border-bottom:1px solid #f5f5f5;
  /* border-top: px solid #f9f9f9; */
  background-position: calc(100% - 20rpx) center;
  background-size: 8px;
  background-repeat: no-repeat;
}
page{
  background: #fff;
}
.nodata_text{
  color: #666666;
  font-size: 14px;  
  text-align: center;  
  height: 50px;
  line-height: 50px;
} 
