  float: right;
  width:20rpx;
  height:30rpx;
} */

/* .head image{
  width:100%;
  height:100%;
} */

.list{
  /* border-bottom: 2rpx solid rgb(218, 217, 217); */
  border-bottom:1px solid #f5f5f5;
  padding-right: 20rpx;
  padding-top:30rpx;
  padding-bottom: 20rpx;
}
.right{
  float: right;
  height:40rpx;
  width:40rpx;
  /* margin-top:20rpx; */
  margin-top:10rpx;
}
.right image{
  width:100%;
  height: 100%;
}
.left{
  margin:0 20rpx;
}
.left>view:nth-child(1) text{
  margin-left:20rpx;
}
.left>view:nth-child(1) text:nth-child(1){
  margin-left:0;
}
.left>view:nth-child(1) text:nth-child(3){
  background: #ff760f;
  border-radius:30rpx;
  font-size: 28rpx;
  padding:0 10rpx;
  color:#fff;
}
.left>view:nth-child(2){
  font-size: 28rpx;
  color:#999;
}