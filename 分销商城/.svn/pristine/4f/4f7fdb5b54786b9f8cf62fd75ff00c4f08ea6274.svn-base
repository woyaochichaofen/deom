.dingdan-body{
  background: #f6f6f6;
}
.nav{
  position:fixed;
  top:0;
  z-index:99;
}
.tab{
  float: left;
  width: 20%;
  text-align: center;
  padding:10rpx 0 20rpx;
}
.topTabSwiper{
  border-bottom: 1px solid #ccc; 
  background: #fff;
  position: fixed;
  top:0;
  left:0;
  z-index: 90;
  width:100%;
  zoom: 1;}
.topTabSwiper:after{
  content: ""; 
  clear: both;
  display: block;
  }
.tabBorer{
  border-bottom: 1px solid #ff6b00; 
  color: #ff7a14;
  }
.swiper{
  width: 100%;
  }
.swiper_con{
  text-align: center; 
  width: 100%; 
  height: 100%;
  margin-top: 10px;
  /* padding: 80rpx 0; */
  }


