image{
  background: pink
}
.searchBar input{
  width: 88%;
  height: 80rpx;
  margin:20rpx 0 25rpx 5%; 
  background: #efefef;
  padding-left: 20rpx;
}
.searchBar view{
  width: 70%;
  height: 80rpx;
  padding-left:32rpx;
  position: absolute;
  background: #efefef;
  left: 5%;
  top: 20rpx;
  z-index: 10;
}
.searchBar view icon{
  position: absolute;
  top: 26rpx;
}
.searchBar view text{
  margin-left: 48rpx;
  font-size: 32rpx;
  color: #888;
  line-height: 80rpx;
}
.searchBar image{
  width: 46rpx;
  height: 50rpx;
  position: absolute;
  top: 40rpx;
  right: 8%;
}


/* box */
.box{
  width: 90%;
  height: 1240rpx;
  margin-left: 5%;
}
/* left */
.Bleft{
  width: 30%;
  height: 100%;
  overflow: hidden;
  margin-left:-6%;
  float: left;
  background: #f5f5f5;
}
.Bleft view{
  width: 100%;
  height: 120rpx;
}
.Bleft view text{
  display: block;
  width: 169rpx;
  height: 60rpx;
  margin-top:30rpx;
  float: right;
  line-height: 60rpx;
  text-align: center;
}
/* 点击样式 */
.BleftView{
  width: 100%;
  height: 120rpx;
  background: white;
}
.BleftView Text{
  display: block;
  width: 169rpx;
  height: 56rpx;
  margin-top:30rpx;
  float: right;
  line-height: 56rpx;
  text-align: center;
  color: #f4a040;
  border-right: 6rpx solid #f4a040;
}
/* 样式集 */

/* right */
.Bright{
  width: 72%;
  height: 100%;
  float: right;
}
.Bright image{
  width: 100%;
  height: 300rpx;
}
.BrightTXT{
  width: 100%;
  height: 80rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 38rpx;
  font-weight: bold;
}
.content{
  width: 100%;
  height: auto;
}
.content view{
  width: 30%;
  height: 200rpx;
  float: left;
  margin:12rpx 0 0 3%;
  text-align: center;
}
.content view image{
  width: 100%;
  height: 140rpx;
  margin-bottom:8rpx; 
}
.content view text{
  font-size: 24rpx;
}