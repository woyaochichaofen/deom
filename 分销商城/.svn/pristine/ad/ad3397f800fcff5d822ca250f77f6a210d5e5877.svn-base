/* pages/user/dai/daipinjia.wxss */
.head-top{
  border-top:2rpx solid #f0f0f0;
  background: #fff;
  border-bottom: 20rpx solid #fafafa;
  height:70rpx;
}
.head{
  border-top:2rpx solid #f0f0f0;
  margin-top: 10px;
}
.head-head{
  background: #fff;
  padding:26rpx 20rpx;
  font-size:30rpx;
}
.head-head-img{
  width:54rpx;
  height:50rpx;
  float: left;
  margin-top:-10rpx;
  margin-right:20rpx;
}
.head-brand{
  height:166rpx;
  background: #fafafa;
  padding:0 20rpx;
}
.head-brand-img{
  float: left;
  width:140rpx;
  height:140rpx;
  background: pink;
  margin-top:16rpx;
}
.head-text{
  padding:20rpx;
  padding-left:156rpx;
  font-size: 28rpx;
}
.head-brand-prc{
  position: relative;
  float: right;
  top:30rpx;
  color:#999;
}
.head-brand-prc>text{
  position: absolute;
  top:40rpx;
  right:0;
}
.head-foot{
  padding:24rpx;
  background: #fff;
  border-bottom:2rpx solid #ececec;
  text-align: right;
}
.head-heji{
  padding-right:20rpx;
}
.head-prc{
  color:#000;
  font-weight: bold;
}

.pinfen-head{
  border-top:30rpx solid #fafafa;
}
.pingfen-text{
  background: #fff;
  padding:2rpx 0 22rpx 20rpx;
  border-bottom: 2rpx solid #cacaca;
}
.boldtext{
  font-size: 32rpx;
}
.pingfen-img{
  width:34rpx;
  height:34rpx;
  margin-left:20rpx;
}
.pingjia{
  color:#999;
  margin-left:30rpx;
}
.footer{
  padding:20rpx;
  background: #fff;
}
.footertextarea{
  height:94rpx;
}
.footerdiv{
  border:2rpx dashed rgb(199, 199, 199);
  width:164rpx;
  height:164rpx;
  text-align: center;
  position: relative;
}
.footerimg{
  width:58rpx;
  height:44rpx;
  margin-top:40rpx;
}
.footertext{
  margin-top:22rpx;
  color:#999;
}
.footerbtnimg{
  position: absolute;
  width:100%;
  height:100%;
  top:0;
  left:0;
}
.footerbtn{
  margin-top:40rpx;
  text-align: center;
  background: #f48864;
  color:#faf8f8;
  padding:24rpx;
  border-radius: 16rpx;
}