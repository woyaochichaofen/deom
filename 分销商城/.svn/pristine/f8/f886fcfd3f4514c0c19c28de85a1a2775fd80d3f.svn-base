.nb-row{
  background: gold;
  height:280rpx;
  margin-top:20rpx;
}
/* .nb-row:nth-child(3){
  margin-top:0;
} */
.nb-left{
  height: 100%;
  float: left;
  width:300rpx;
}
.nb-right{
  height:100%;
  width:100%;
  background: #fbede4;
  text-align: center;
}
.nb-text1{
  padding:18rpx;
  font-size: 32rpx;
  padding-left:310rpx;
  line-height:46rpx;
  height:140rpx;
}
.nb-text1>view{
  overflow: hidden;
  height:90rpx;
}
.nb-text2{
  font-size: 30rpx;
  line-height: 10rpx;
  margin-top:10rpx;
}
.nb-text2>text:nth-child(1){
  color:#d3403f;
}
.nb-text2>text:nth-child(2){
  font-size: 26rpx;
  text-decoration: line-through;
  color:#999;
  margin-left:20rpx;
}
.nb-btn{
  background: #ff750e;
  display: inline-block;
  margin-top:20rpx;
  font-size:30rpx;
  padding:16rpx 20rpx;
  border-radius: 10rpx;
  color: #fff;
}

.floatdiv{
  position: fixed;
  top:0;
  left:0;
  height:100%;
  width:100%;
  background: rgba(0, 0, 0,0.8);
  text-align: center;
}
.floatimg{
  width:486rpx;
  height:588rpx;
  border-radius: 20rpx;
  overflow: hidden;
  margin: 0 auto;
  margin-top:290rpx;
  background-size:100%;
  background-repeat:no-repeat;
  background-position:center center;  
}
.floatbtn{
  height:64rpx;
  width:64rpx;
  display: block;
  margin:0 auto;
  margin-top:30rpx;
}